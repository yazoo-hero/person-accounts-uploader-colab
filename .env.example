# Calabrio API Configuration
CALABRIO_API_BASE_URL=https://your-calabrio-instance.com
CALABRIO_API_KEY=your-api-key-here
CALABRIO_API_SECRET=your-api-secret-here

# Application Configuration
APP_ENV=development
APP_DEBUG=true
APP_PORT=8050

# Data Paths (optional, defaults to ./data)
DATA_DIR=./data
CONFIG_DIR=./config

# Logging
LOG_LEVEL=INFO
LOG_FILE=app.log

# Jupyter/Colab Settings
JUPYTER_DASH_MODE=inline